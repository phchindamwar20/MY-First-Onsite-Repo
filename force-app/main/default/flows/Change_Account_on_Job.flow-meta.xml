        <name>forT_Emailrecipients</name>
        <dataType>String</dataType>
        <expression>IF(
ISBLANK({!JobId.BD_Account_Manager__c}),&quot;&quot;,
{!JobId.BD_Account_Manager__r.Email} &amp; &quot;,&quot;)
&amp;
IF(
ISBLANK({!JobId.BD_Account_Manager_2__c}),&quot;&quot;,
{!JobId.BD_Account_Manager_2__r.Email} &amp; &quot;,&quot;)
&amp;
IF(
ISBLANK({!JobId.BD_Account_Manager_3__c}),&quot;&quot;,
{!JobId.BD_Account_Manager_3__r.Email} &amp;&quot;,&quot;)
&amp;
IF(
ISBLANK({!JobId.Project_Director__c}),&quot;&quot;,
{!JobId.Project_Director__r.Email} &amp;&quot;,&quot;)
&amp;
&quot;ithelp@firstonsite.com&quot;</expression>
    </formulas>
    <formulas>
        <name>propertyaccountnameformula</name>
        <dataType>String</dataType>
        <expression>{!propertyAccRecVar.ShippingStreet} &amp; &quot; &quot;
&amp; {!propertyAccRecVar.ShippingCity} &amp; &quot; &quot;
&amp; TEXT({!propertyAccRecVar.ShippingStateCode}) &amp; &quot; &quot;
&amp; {!propertyAccRecVar.ShippingPostalCode} 
&amp; IF(!ISBLANK({!propertyAccRecVar.Property_Name__c}),&quot;-&quot; &amp; {!propertyAccRecVar.Property_Name__c},&quot;&quot;) 
