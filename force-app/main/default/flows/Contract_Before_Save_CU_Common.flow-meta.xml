<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <decisions>
        <name>Any_Contract_Executed</name>
        <label>Any Contract Executed?</label>
        <locationX>182</locationX>
        <locationY>587</locationY>
        <defaultConnectorLabel>Executed_No</defaultConnectorLabel>
        <rules>
            <name>Executed_Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>forB_ExecutedStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Date_Executed</targetReference>
            </connector>
            <label>Executed_Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>CO_or_SOWC_has_file</name>
        <label>CO or SOWC has file</label>
        <locationX>182</locationX>
        <locationY>287</locationY>
        <defaultConnector>
            <targetReference>Any_Contract_Executed</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>File_No</defaultConnectorLabel>
        <rules>
            <name>File_Yes</name>
            <conditionLogic>1 AND 2 AND (3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Number_of_Files__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Number_of_Files__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Client_Contract_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Change Order</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Client_Contract_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Statement of Work Complete</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Status_Complete</targetReference>
            </connector>
            <label>File_Yes</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>forB_DataAdmin</name>
        <dataType>Boolean</dataType>
        <expression>{!$Setup.DataAdmin__c.TurnOffFlow__c}</expression>
    </formulas>
    <formulas>
        <name>forB_ExecutedStatus</name>
        <dataType>Boolean</dataType>
        <expression>AND(
ISCHANGED({!$Record.Status}),
CONTAINS(TEXT({!$Record.Status}),&quot;Executed&quot;) 
)</expression>
    </formulas>
    <formulas>
        <name>forD_Today</name>
        <dataType>Date</dataType>
        <expression>TODAY()</expression>
    </formulas>
    <formulas>
        <name>forDT_Now</name>
        <dataType>DateTime</dataType>
        <expression>NOW()</expression>
    </formulas>
    <interviewLabel>Contract - Before Save -Common {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Contract - Before Save CU - Common</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Date_Executed</name>
        <label>Date Executed</label>
        <locationX>50</locationX>
        <locationY>695</locationY>
        <connector>
            <targetReference>Date_Fully_Executed</targetReference>
        </connector>
        <inputAssignments>
            <field>Date_Executed__c</field>
            <value>
                <elementReference>forDT_Now</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Date_Fully_Executed</name>
        <label>Date Fully Executed</label>
        <locationX>50</locationX>
        <locationY>803</locationY>
        <inputAssignments>
            <field>Date_Fully_Executed__c</field>
            <value>
                <elementReference>$Record.Date_Executed__c</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Status_Complete</name>
        <label>Status Complete</label>
        <locationX>50</locationX>
        <locationY>395</locationY>
        <connector>
            <targetReference>Any_Contract_Executed</targetReference>
        </connector>
        <inputAssignments>
            <field>Date_Fully_Executed__c</field>
            <value>
                <elementReference>forD_Today</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Status</field>
            <value>
                <stringValue>Complete</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>56</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>CO_or_SOWC_has_file</targetReference>
        </connector>
        <filterFormula> {!$Setup.DataAdmin__c.TurnOffFlow__c} = false</filterFormula>
        <object>Contract</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
