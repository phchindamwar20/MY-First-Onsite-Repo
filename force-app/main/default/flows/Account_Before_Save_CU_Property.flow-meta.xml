            </connector>
            <label>Bill To = Property</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Call_Center_Created_Property</name>
        <label>Is Call Center Created Property?</label>
        <locationX>226</locationX>
        <locationY>695</locationY>
        <defaultConnector>
            <targetReference>Is_New_Property_or_Changed_to_Property_Record_Type</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Call_Center_Created_Property</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Owner.UserRole.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Call_Center</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Owner.Profile.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Integration</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Property_Owner_to_Parent_Owner</targetReference>
            </connector>
            <label>Call Center Created Property</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_New_Property_or_Changed_to_Property_Record_Type</name>
        <label>Is New Property or Changed to Property Record Type?</label>
        <locationX>380</locationX>
        <locationY>1079</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>forB_isNew</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordTypeId</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Name_Active_DD</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
