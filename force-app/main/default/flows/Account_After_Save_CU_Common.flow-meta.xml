    <formulas>
        <name>forT_MatchedName</name>
        <dataType>String</dataType>
        <expression>IF( AND( {!$Record.RecordType.DeveloperName}= &quot;Property&quot;, {!$Record.Active__c} = true), TEXT({!$Record.ShippingStateCode}) &amp; &quot;_&quot; &amp; {!$Record.ShippingCity} &amp; &quot;_&quot; &amp; {!$Record.ShippingStreet} &amp; &quot;_&quot; &amp; {!$Record.Ultimate_Parent__c}, IF( AND( {!$Record.RecordType.DeveloperName} = &quot;Property&quot;, {!$Record.Active__c} = false ), TEXT({!$Record.ShippingCountryCode}) &amp; &quot;_&quot; &amp; {!$Record.ShippingCity} &amp; &quot;_&quot; &amp; {!$Record.ShippingStreet} &amp; &quot;_&quot; &amp; {!$Record.Ultimate_Parent__c} &amp; &quot;_Inactive-&quot; &amp; TEXT({!$Record.Inactive_Date__c}), {!$Record.Name}) )</expression>
    </formulas>
