            </connector>
            <label>Yes?</label>
        </rules>
    </decisions>
    <description>STAR-6504 Sets the exception payment field on employee commission.</description>
    <environments>Default</environments>
    <formulas>
        <name>forC_ActualCommissionAmount</name>
        <dataType>Currency</dataType>
        <expression>IF(TEXT({!$Record.Job_Commission__r.Actual_Commissionable__c})={!con_Yes} &amp;&amp; TEXT({!$Record.Job_Commission__r.Actual_Commissionable__c}) = {!con_Yes}, 
  (
        {!$Record.Job_Commission__r.Actual_Deposit_1st_Payment_Incentive__c} + 
        {!$Record.Job_Commission__r.Actual_Quick_Collection_Incentive__c} +
        {!$Record.Job_Commission__r.Actual_Slow_Collection_Incentive__c}
)
    /100.0000 * {!$Record.Job_Commission__r.Split_Multiplier__c} * {!$Record.Job_Commission__r.Job_Amount__c} * 
  {!$Record.Individual_Split_Percentage__c}/ 100.0000 ,
 0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>forC_ActualDepositFirstPaymentAmount</name>
        <dataType>Currency</dataType>
        <expression>IF(TEXT({!$Record.Job_Commission__r.Actual_DepositFirst_Payment_Met__c})={!con_Yes} &amp;&amp; TEXT({!$Record.Job_Commission__r.Actual_Commissionable__c}) = {!con_Yes},  
     {!$Record.Individual_Split_Percentage__c}/100.0000 * {!$Record.Job_Commission__r.Job_Amount__c} * 
    {!$Record.Job_Commission__r.Split_Multiplier__c} * 
   {!$Record.Job_Commission__r.Actual_Deposit_1st_Payment_Incentive__c} / 100.0000
, 0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
